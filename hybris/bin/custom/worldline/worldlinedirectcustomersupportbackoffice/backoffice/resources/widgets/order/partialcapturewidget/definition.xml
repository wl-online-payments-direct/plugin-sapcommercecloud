<widget-definition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:noNamespaceSchemaLocation="http://www.hybris.com/schema/cockpitng/widget-definition.xsd"
                   id="com.worldline.direct.widgets.order.partialcapturewidget">
    <name>Partial Capture Widget</name>
    <description>Partial Capture Widget built for Worldline plugin</description>
    <defaultTitle>Partial Capture Widget</defaultTitle>
    <author>hybris</author>
    <version>1.0</version>

    <view src="partialcapturewidget.zul" />
    <keywords>
        <keyword>OMS</keyword>
    </keywords>

    <sockets>
        <input type="de.hybris.platform.core.model.order.OrderModel" id="inputObject" />
        <output type="java.lang.String" id="confirmpaymentcapture" />
    </sockets>

    <controller class="com.worldline.direct.widgets.worldlinepartialcapture.WorldlinePartialCaptureController" />
